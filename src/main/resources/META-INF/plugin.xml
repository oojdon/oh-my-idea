<idea-plugin url="http://plugins.jetbrains.com/plugin/164">
    <name>Oh My IDEA</name>
    <id>oh-my-idea</id>
    <description>write java without mouse</description>
    <version>1.0</version>
    <vendor>JetBrains</vendor>

    <depends>com.intellij.modules.lang</depends>

    <application-components>
        <component>
            <implementation-class>com.codingbaby.ohmyidea.OhPlugin</implementation-class>
        </component>
    </application-components>


    <actions>
        <action id="ShortcutKeyAction" class="com.codingbaby.ohmyidea.ShortcutKeyAction" text="Shortcuts"/>


        <action id="MotionFirstLine" class="com.codingbaby.ohmyidea.action.MotionGotoLineFirstAction" text="First Line"/>
        <action id="MotionLastLine" class="com.codingbaby.ohmyidea.action.MotionGotoLineLastAction" text="Last Line"/>
        <action id="MotionToLine" class="com.codingbaby.ohmyidea.action.MotionToLineAction" text="Motion To Line"/>


        <action id="MotionToMatchChar" class="com.codingbaby.ohmyidea.action.MotionToMatchCharAction" text="Motion To Match"/>

        <action id="ShowCommandAction" class="com.codingbaby.ohmyidea.action.ShowCommandAction" text="Show Command"/>

        <action id="LoadScriptAction" class="com.codingbaby.ohmyidea.action.LoadScriptAction" text="Load Script"/>

        <action id="ScrollToTop" class="com.codingbaby.ohmyidea.action.ScrollToScreenFirstAction" text="Scroll Top"/>

        <action id="NotQuickInsert" class="com.codingbaby.ohmyidea.action.NotQuickInsertAction" text="Insert Not Quick"/>

        <action id="ShowHelpDialog" class="com.codingbaby.ohmyidea.action.ShowHelpDialogAction" text="Show Help Dialog"/>

        <action id="EditCodeTemplate" class="com.codingbaby.ohmyidea.action.CodeTemplateEditAction" text="Edit Code Template"/>

        <action id="EditorDeleteToFileEnd" class="com.codingbaby.ohmyidea.action.DeleteToFileEndAction" text="Delete To File End"/>

        <action id="RepeatCurrentAction" class="com.codingbaby.ohmyidea.action.RepeatCurrentAction" text="Repeat Current Action"/>

        <action id="ShowUIToggleActions" class="com.codingbaby.ohmyidea.action.ShowUIToggleActions" text="Control UIToggle Actions"/>

    </actions>


</idea-plugin>
